{
  "name": "reactjs-components",
  "version": "0.14.0-beta.1",
  "description": "React UI Components by Mesosphere",
  "author": {
    "name": "Mesosphere Frontend Team",
    "email": "web-public@mesosphere.com",
    "url": "http://www.mesosphere.com"
  },
  "engines": {
    "node": "^4.x.x",
    "npm": "^2.x.x"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mesosphere/reactjs-components"
  },
  "license": "Apache-2.0",
  "files": [
    "main.js",
    "lib",
    "npm-shrinkwrap.json"
  ],
  "main": "main.js",
  "dependencies": {
    "classnames": "~2.2.3"
  },
  "devDependencies": {
    "babel-cli": "~6.4.0",
    "babel-core": "~6.4.0",
    "babel-eslint": "~5.0.0-beta6",
    "babel-jest": "~6.0.1",
    "babel-loader": "~6.2.1",
    "babel-plugin-transform-runtime": "~6.4.0",
    "babel-preset-es2015": "~6.3.13",
    "babel-preset-react": "~6.3.13",
    "babel-runtime": "~6.3.19",
    "browser-sync": "~2.11.0",
    "canvas-ui": "~0.2.1",
    "envify": "~3.4.0",
    "eslint": "~1.10.3",
    "eslint-plugin-react": "~3.15.0",
    "glob": "~6.0.4",
    "gulp": "~3.9.0",
    "gulp-autoprefixer": "~3.1.0",
    "gulp-concat": "~2.6.0",
    "gulp-connect": "~2.3.1",
    "gulp-eslint": "~1.1.1",
    "gulp-less": "~3.0.5",
    "gulp-minify-css": "~1.2.3",
    "gulp-replace": "~0.5.3",
    "gulp-sourcemaps": "~1.6.0",
    "gulp-uglify": "~1.5.1",
    "gulp-util": "~3.0.7",
    "jest-cli": "~0.8.2",
    "less-color-lighten": "0.0.1",
    "node-libs-browser": "~0.5.2",
    "react": "~0.14.6",
    "react-addons-css-transition-group": "~0.14.6",
    "react-addons-test-utils": "~0.14.6",
    "react-dom": "~0.14.6",
    "react-gemini-scrollbar": "~2.1.0",
    "source-map-loader": "~0.1.5",
    "transform-loader": "~0.2.3",
    "webpack": "~1.12.11"
  },
  "peerDependencies": {
    "canvas-ui": "~0.2.x",
    "react": "~0.14.x",
    "react-addons-css-transition-group": "~0.14.x",
    "react-dom": "~0.14.x",
    "react-gemini-scrollbar": "~2.1.x"
  },
  "scripts": {
    "clean": "rm -rf dist docs/dist lib",
    "dist": "npm run clean && NODE_ENV='production' ./node_modules/.bin/gulp docs:dist",
    "predist-src": "rm -rf ./lib",
    "dist-src": "cp -r ./src ./lib && find ./lib -name '__tests__' -type d -exec rm -r '{}' + && ./node_modules/.bin/babel lib --out-dir lib",
    "livereload": "NODE_ENV='development' ./node_modules/.bin/gulp docs:livereload",
    "npm-publish": "npm publish ./",
    "test": "jest",
    "release": "npm run clean && npm run test && ./node_modules/.bin/gulp eslint && npm run dist-src && npm publish ./"
  },
  "jest": {
    "globals": {
      "__DEV__": true
    },
    "scriptPreprocessor": "./node_modules/babel-jest",
    "testFileExtensions": [
      "js"
    ],
    "moduleFileExtensions": [
      "js",
      "json"
    ],
    "setupTestFrameworkScriptFile": "jest/setupTestFramework.js",
    "setupEnvScriptFile": "jest/setupEnv.js",
    "modulePathIgnorePatterns": [
      "/tmp/",
      "/node_modules/",
      "/.module-cache/"
    ],
    "unmockedModulePathPatterns": [
      "./src/Mixin",
      "./src/Util",
      "babel-runtime",
      "classnames",
      "react",
      "react-dom",
      "react-addons-test-utils",
      "fbjs"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ]
  }
}
